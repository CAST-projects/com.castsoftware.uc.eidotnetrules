    <METRIC_ID>1118002</METRIC_ID>
    <DESCRIPTION_TYPE_ID>5</DESCRIPTION_TYPE_ID>
    <LANGUAGE>ENGLISH</LANGUAGE>
    <METRIC_DESCRIPTION>public class Fruit
{
  protected Season ripe;
  protected Color flesh;

